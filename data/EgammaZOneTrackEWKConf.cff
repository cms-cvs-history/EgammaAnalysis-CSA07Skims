#
# Reuse EWK modules, but reconfigure for higher mass cut
#

# This is the only real change
module EgammaZOneTrack = zToEEOneTrack from "ElectroWeakAnalysis/ZReco/data/zToEEOneTrack.cfi"
replace EgammaZOneTrack.massMin = 60.


# change the input tags to the changed module
module EgammaZOneTrackGenParticlesMatch = zToEEOneTrackGenParticlesMatch from  "ElectroWeakAnalysis/ZReco/data/zToEEOneTrackGenParticlesMatch.cfi"
replace EgammaZOneTrackGenParticlesMatch.src = EgammaZOneTrack

module EgammaZOneTrackFilter = zToEEOneTrackFilter from "ElectroWeakAnalysis/ZReco/data/zToEEOneTrackFilter.cfi"
replace EgammaZOneTrackFilter.src = EgammaZOneTrack
