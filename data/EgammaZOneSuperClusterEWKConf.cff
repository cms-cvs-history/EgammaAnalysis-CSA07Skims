# change the input tags to the changed module
module EgammaZOneSuperClusterGenParticlesMatch = zToEEOneSuperClusterGenParticlesMatch from  "ElectroWeakAnalysis/ZReco/data/zToEEOneSuperClusterGenParticlesMatch.cfi"
replace EgammaZOneSuperClusterGenParticlesMatch.src = EgammaZOneSuperCluster

module EgammaZOneSuperClusterFilter = zToEEOneSuperClusterFilter from "ElectroWeakAnalysis/ZReco/data/zToEEOneSuperClusterFilter.cfi"
replace EgammaZOneSuperClusterFilter.src = EgammaZOneSuperCluster
